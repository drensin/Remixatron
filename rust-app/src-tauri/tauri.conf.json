{
    "productName": "Remixatron",
    "version": "0.9.1",
    "identifier": "com.remixatron.dev",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../src"
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": {
            "libs/linux/*": "libs/linux/",
            "libs/macos/*": "libs/macos/",
            "libs/win/*": "libs/win/",
            "models/*": "models/"
        }
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "Remixatron",
                "width": 1024,
                "height": 768
            }
        ],
        "security": {
            "csp": null
        }
    }
}