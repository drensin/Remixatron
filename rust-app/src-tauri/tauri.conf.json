{
    "productName": "remixatron",
    "version": "0.0.0",
    "identifier": "com.remixatron.dev",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../src"
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "Remixatron",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    }
}